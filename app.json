{
  "expo": {
    "name": "THime",
    "description": "Tracking Time for Human Resources",
    "slug": "thime",
    "privacy": "unlisted",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "5",
    "orientation": "portrait",
    "icon": "./assets/green/icon-with-bg.png",
    "splash": {
      "image": "./assets/green/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EDF9F6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alexbordin.thime"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#EDF9F6"
    },
    "android": {
      "package": "com.alexbordin.thime",
      "permissions": [],
      "versionCode": 5,
      "adaptiveIcon": {
        "backgroundColor": "#EDF9F6",
        "foregroundImage": "./assets/green/icon.png"
      },
      "googleServicesFile": "./google-services.json"
    }
  }
}
